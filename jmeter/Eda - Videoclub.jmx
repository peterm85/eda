        <stringProp name="filename"></stringProp>
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="script">log.info(&quot;Before execution&quot;)
log.info(&quot;Threads: ${threads}&quot;)
log.info(&quot;Errors: ${not-available-errors}&quot;)

long threads = Long.parseLong(vars.get(&quot;threads&quot;))
long errors = Long.parseLong(vars.get(&quot;not-available-errors&quot;))

number = threads - errors
vars.put(&quot;stock&quot;, String.valueOf(number))

log.info(&quot;Stock updated: &quot;+vars.get(&quot;stock&quot;))</stringProp>
